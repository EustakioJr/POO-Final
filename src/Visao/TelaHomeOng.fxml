<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controle.ControleTelaHomeOng">
    <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="2.0">
        <Menu mnemonicParsing="false" text="Menu">
            <MenuItem fx:id="opcaoHome" mnemonicParsing="false" onAction="#irHome" text="Home" />
            <MenuItem fx:id="opcaoLogout" mnemonicParsing="false" onAction="#logout" text="Logout" />
        </Menu>
    </MenuBar>
    <Button fx:id="botaoListaClinica" layoutX="224.0" layoutY="104.0" mnemonicParsing="false" onAction="#irListaClinica" prefHeight="25.0" prefWidth="153.0" text="Listar Clinicas Veterinarias" />
    <Button fx:id="botaoListaAnimal" layoutX="190.0" layoutY="155.0" mnemonicParsing="false" onAction="#irListaAnimal" text="Listar animais disponiveis para adoção" />
    <Button fx:id="botaoLogout" layoutX="273.0" layoutY="352.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
    <Button fx:id="botaoAlterar" layoutX="241.0" layoutY="289.0" mnemonicParsing="false" onAction="#irAlterar" text="Alterar meus dados" />
   <Button fx:id="botaoAdicionaAnimal" layoutX="214.0" layoutY="207.0" mnemonicParsing="false" onAction="#irCadastroAnimal" text="Adicionar animal para adoção" />
</AnchorPane>
